  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/redradix/redux-base-app.git"
  },
  "scripts": {
    "start": "npm run clean && NODE_ENV=development nodemon ./src/server.js -w ./src/server.js",
    "start-prod": "NODE_ENV=production node ./src/server.js",
    "build": "NODE_ENV=production webpack -p --progress --colors --config ./config/webpack.config.prod",
    "test": "NODE_ENV=test mocha --recursive src/test --compilers js:babel-core/register --require ./src/test/setup.js",
    "test:modules": "NODE_ENV=test mocha --recursive ./src/modules/ --compilers js:babel-core/register --require ./src/test/setup.js",
    "test:watch": "npm test -- --watch",
    "lint": "eslint ./src/**",
    "clean": "rm -f public/index.html && rm -rf public/js"
  },
  "dependencies": {
    "deep-freeze": "0.0.1",
    "dom-scroll-into-view": "1.0.1",
    "i18next": "2.0.23",
    "i18next-browser-languagedetector": "0.0.14",
    "i18next-xhr-backend": "0.3.0",
    "isomorphic-fetch": "2.2.1",
    "react": "15.0.0",
    "react-dom": "15.0.0",
    "react-i18next": "1.3.0",
    "react-redux": "4.4.1",
    "react-router": "2.0.1",
    "react-router-redux": "4.0.2",
    "redux": "3.3.1",
    "redux-form": "4.1.6",
    "redux-thunk": "1.0.3",
    "reselect": "2.0.2"
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-2": "6.3.13",
    "css-loader": "0.23.1",
    "dotenv": "2.0.0",
    "eslint": "2.7.0",
    "eslint-plugin-babel": "3.2.0",
    "eslint-plugin-mocha": "2.0.0",
    "eslint-plugin-react": "4.3.0",
    "expect": "1.6.0",
    "express": "4.13.3",
    "html-webpack-plugin": "2.7.1",
    "jsdom": "8.0.1",
    "mocha": "2.2.5",
    "nock": "7.0.2",
    "node-libs-browser": "0.5.2",
    "nodemon": "1.8.1",
    "react-addons-test-utils": "15.0.0",
    "react-transform-hmr": "1.0.0",
    "style-loader": "0.13.0",
    "webpack": "1.12.13",
    "webpack-dev-middleware": "1.5.1",
    "webpack-hot-middleware": "2.6.4"
  },
  "engines": {
    "node": ">=4.2.6",
    "npm": ">=2.14"
  }
}
